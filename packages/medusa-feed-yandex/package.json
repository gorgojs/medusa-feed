{
  "name": "@medurajs/medusa-feed-yandex",
  "version": "0.0.1",
  "description": "A Medusa plugin that exports product data in Yandex Market YML feed format",
  "license": "MIT",
  "files": [
    ".medusa/server"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./workflows": "./.medusa/server/src/workflows/index.js",
    "./.medusa/server/src/modules/*": "./.medusa/server/src/modules/*/index.js",
    "./modules/*": "./.medusa/server/src/modules/*/index.js",
    "./providers/*": "./.medusa/server/src/providers/*/index.js",
    "./*": "./.medusa/server/src/*.js",
    "./admin": {
      "import": "./.medusa/server/src/admin/index.mjs",
      "require": "./.medusa/server/src/admin/index.js",
      "default": "./.medusa/server/src/admin/index.js"
    }
  },
  "keywords": [
    "medusa",
    "plugin",
    "medusa-plugin-other",
    "medusa-plugin",
    "medusa-v2",
    "ecommerce",
    "yandex",
    "yandex market",
    "yml",
    "yml feed",
    "product feed",
    "export",
    "catalog export",
    "yandex yml",
    "localization",
    "marketplace"
  ],
  "scripts": {
    "build": "medusa plugin:build",
    "dev": "yarn predev && medusa plugin:develop",
    "predev": "medusa plugin:publish",
    "prepublishOnly": "medusa plugin:build"
  },
  "dependencies": {
    "yandex-market-language": "^0.5.0"
  },
  "devDependencies": {
    "@medusajs/admin-sdk": "^2.8.3",
    "@medusajs/cli": "^2.8.3",
    "@medusajs/framework": "^2.8.3",
    "@medusajs/icons": "^2.8.3",
    "@medusajs/medusa": "^2.8.3",
    "@medusajs/test-utils": "^2.8.3",
    "@medusajs/ui": "^4.0.13",
    "@mikro-orm/cli": "6.4.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@swc/core": "1.5.7",
    "@types/node": "^22.13.17",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.2.25",
    "awilix": "^8.0.1",
    "pg": "^8.14.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "@medusajs/admin-sdk": "^2.8.3",
    "@medusajs/cli": "^2.8.3",
    "@medusajs/framework": "^2.8.3",
    "@medusajs/icons": "^2.8.3",
    "@medusajs/medusa": "^2.8.3",
    "@medusajs/test-utils": "^2.8.3",
    "@medusajs/ui": "^4.0.13",
    "@mikro-orm/cli": "6.4.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "awilix": "^8.0.1",
    "pg": "^8.13.0"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@4.9.1"
}
